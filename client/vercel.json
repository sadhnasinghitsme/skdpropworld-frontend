{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite",
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "skdpropworld.vercel.app"
        }
      ],
      "destination": "https://www.skdpropworld.com/$1",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "skdpropworld.com"
        }
      ],
      "destination": "https://www.skdpropworld.com/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "https://https-www-skdpropworld-com.onrender.com/sitemap.xml"
    },
    {
      "source": "/api/:path*",
      "destination": "https://https-www-skdpropworld-com.onrender.com/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    
  ]
}