{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite",
  "redirects": [
    {
      "source": "http://skdpropworld.com/:path*",
      "destination": "https://www.skdpropworld.com/:path*",
      "permanent": true
    },
    {
      "source": "http://www.skdpropworld.com/:path*",
      "destination": "https://www.skdpropworld.com/:path*",
      "permanent": true
    },
    {
      "source": "https://skdpropworld.com/:path*",
      "destination": "https://www.skdpropworld.com/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "https://skd-backend.onrender.com/sitemap.xml"
    },
    {
      "source": "/api/:path*",
      "destination": "https://skd-backend.onrender.com/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Prerender-Token",
          "value": "QHhhrvIPvM5gm4fHnmaT"
        }
      ]
    }
  ]
}
